<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Customer Additional Attributes </title>
    </head>
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-customfield-index-link" ifconfig="regform/active/enable">
                <arguments>
                    <argument name="path" xsi:type="string">regform/additional</argument>
                    <argument name="label" xsi:type="string" translate="true">Customer Additional Attributes</argument>
                    <argument name="sort" xsi:type="string">50</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
 